Package: dfrr
Type: Package
Title: Dichotomized Functional Response Regression
Version: 0.1.4
Authors@R: c(
      person("Fatemeh", "Asgari", email = "ft.asgari@sci.ui.ac.ir", role = c("aut", "cre")),
